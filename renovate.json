{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "groupName": "all dependencies",
  "groupSlug": "all",
  "lockFileMaintenance": {
    "enabled": false
  },
  "packageRules": [
    {
      "groupName": "all dependencies",
      "groupSlug": "all",
      "matchPackageNames": [
        "*"
      ]
    },
    {
      "matchPackageNames": ["ghcr.io/immich-app/postgres"],
      "allowedVersions": ["14"]
    }
  ],
  "separateMajorMinor": false
}
